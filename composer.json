{
  "name": "pixelfederation/druid-php",
  "description": "PHP compliant driver for Druid Database",
  "homepage": "https://git.pixelfederation.com/shared/Vitess.Driver.PHP",
  "license": "BSD-3-Clause",
  "type": "library",
  "keywords": [
    "Druid",
    "driver"
  ],
  "authors": [
    {
      "name": "Tomas Mihalicka",
      "email": "tmihalicka@pixelfederation.com",
      "homepage": "https://github.com/pixelfederation"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "^6.1",
    "jms/serializer": "^1.1"
  },
  "require-dev": {
    "symfony/var-dumper": "^3.0@dev",
    "phpunit/phpunit": "^5.1",
    "squizlabs/php_codesniffer": "2.*",
    "phpmd/phpmd": "@stable"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Druid\\": "src/Druid/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Druid\\": "tests/Druid/"
    }
  }
}
